{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/build/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "outputDirectory": "frontend/build"
}
